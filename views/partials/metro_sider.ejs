<div class="kt-aside  kt-aside--fixed  kt-grid__item kt-grid kt-grid--desktop kt-grid--hor-desktop" id="kt_aside">
	<div class="kt-aside__brand kt-grid__item " id="kt_aside_brand">
		<div class="kt-aside__brand-logo">
			<a href="javascript:void(0)" class="home-page-linker" id="logobrandlinker">
				<img id="imagelogobrand" alt="Logo" src="/img/app_logo_title.png" />
			</a>
		</div>
		<div class="kt-adside_brand-logo-short">
			<a href="javascript:void(0)" class="home-page-linker" id="logoshortlinker">
				<img id="imagelogobrandshort" alt="Logo" src="/img/app_logo_short.png" />
			</a>
		</div>
		<div class="kt-aside__brand-tools">
			<button class="kt-aside__brand-aside-toggler" id="kt_aside_toggler">
				<span><i class="la la-angle-double-left"></i></span>
				<span><i class="la la-angle-double-right"></i></span>
			</button>
		</div>
	</div>
	<div class="kt-aside-menu-wrapper kt-grid__item kt-grid__item--fluid" id="kt_aside_menu_wrapper">
		<div id="kt_aside_menu" class="kt-aside-menu " data-ktmenu-vertical="1" data-ktmenu-scroll="1" data-ktmenu-dropdown-timeout="500">
			<ul class="kt-menu__nav py-0">
				<li class="kt-menu__item " aria-haspopup="true">
					<span id="menulinkspaner" class="kt-menu__link ">
						<span class="kt-menu__link-text">
							<div id="sidebarheader" class="sidebar-header">
								<div class="input-group-search has-search">
									<span id="searchspan" class="fa fa-search form-control-search" aria-hidden="true"></span>
									<input type="text" id="sidemenusearchtext" class="form-control2 form-control-input-search input-md" placeholder="<%=label.get('menusearch_title','Search')%>" />
								</div>
							</div>
						</span>
					</span>
				</li>
				<li id="sidebar-home" class="kt-menu__item  kt-menu__item--active" aria-haspopup="true">
					<a href="javascript:void(0)" class="kt-menu__link home-page-linker" id="homepagelinker">
						<span class="kt-menu__link-icon">
							<i class="fa fa-home"></i>
						</span>
						<span class="kt-menu__link-text"><%=label.get('homepage_title','Home')%></span>
					</a>
				</li>
			</ul>
			<div id="sidebarlayer">
			</div>
			<div class="spanbuildversion" id="spanbuildversion">
				Build : <%=meta.version%>
			</div>
		</div>
	</div>
</div>
