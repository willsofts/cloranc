<!DOCTYPE html>
<html lang="en">
	<head>
		<title>Assure</title>		
		<%- include('../asset/meta') %>		
		<%- include('../asset/stylesmain') %>		
		<%- include('../asset/scriptsmain') %>		
		<% if(!(meta.info?.appstype == "W")) { %>		
		<script type="text/javascript" src="<%=meta.cdn_url%>/js/msal-browser.js" defer></script>
		<script type="text/javascript" src="<%=meta.cdn_url%>/js/auth.js" defer></script>
		<% } %>
		<script type="text/javascript" src="<%=meta.cdn_url%>/js/index.js" defer></script>
	</head>
	<body class="portalbody portalbody-off body-login">
		<nav id="navigatebar" class="navbar navbar-expand navigatebar-top fa-hidden">
			<%- include('../partials/main_header') %>		
		</nav>
		<nav id="sidebarmenu" class="sidebar sidebar-menu main sidebar-hide fliph navbar-expand fa-hidden">
			<div id="sidebarheader" class="sidebar-header">
				<div class="input-group-search has-search">
					<span id="searchspan" class="fa fa-search form-control-search" aria-hidden="true"></span>
					<input type="text" id="sidemenusearchtext" class="form-control form-control-input-search input-md" placeholder="Search" />
				</div>
			</div>
			<div id="sidebarlayer" class="sidebar-layer sidebar left">				
			</div>
		</nav>
		<%- include('../partials/page_login') %>		
		<div id="fsworkinglayer" class="working-class working-control-class">
			<div id="pagecontainer" class="pt-pager"></div> 
			<iframe id="workingframe" name="workingframe" width="100%" class="working-frame" title=""></iframe>
		</div>
		<% if(meta.info?.chat) {%>
		<div id="fschatinglayer" class="chating-class chating-control-class">
			<iframe id="chatingframe" name="chatingframe" width="100%" class="chating-frame" title="Chating"></iframe>
		</div>
		<%}%>	
		<%- include('../partials/page_waiting') %>		
	</body>
</html>